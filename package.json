{
  "name": "bitt",
  "version": "0.1.5",
  "description": "An extremely light zero dependency runtime javascript framework.",
  "main": "dist/bitt.js",
  "repository": "https://github.com/Andr3wRiv3rs/bitt",
  "module": "true",
  "author": {
    "name": "Andres Sweeney-Rios",
    "email": "andrewarivers@gmail.com",
    "url": "https://poisonapple.dev"
  },
  "type": "module",
  "scripts": {
    "dist": "rollup -c --environment format:esm && rollup -c --format cjs --environment format:cjs"
  },
  "exports": {
    ".": {
      "require": "./dist/cjs.js",
      "default": "./dist/esm.js"
    }
  },
  "license": "MIT",
  "devDependencies": {
    "rollup": "^2.23.1",
    "rollup-plugin-terser": "^7.0.0"
  }
}
